<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserMatching">


    <Button
        android:id="@+id/matching_chat_go"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginTop="667dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="10dp"
        android:text="채팅"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="668dp" />

    <Button
        android:id="@+id/matching"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginTop="667dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="10dp"
        android:text="매칭"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="668dp" />

    <Button
        android:id="@+id/matching_setting_go"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="667dp"
        android:layout_marginBottom="10dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="7dp"
        android:text="설정"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="315dp" />

    <Button
        android:id="@+id/matching_start"
        android:layout_width="321dp"
        android:layout_height="55dp"
        android:text="매칭 시작"
        android:textSize="24sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="493dp" />

    <TextView
        android:id="@+id/matching_explanation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="딥러닝이 찾은 내 이상형은 누구일까?"
        android:textColor="@color/black"
        android:textSize="16sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="466dp" />

    <ImageView
        android:id="@+id/matching_image"
        android:layout_width="317dp"
        android:layout_height="302dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="62dp"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/matching_home_go"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginTop="667dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="10dp"
        android:text="홈"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="15dp" />

</androidx.constraintlayout.widget.ConstraintLayout>